{
  "name": "ng-redux-example",
  "version": "1.0.0",
  "description": "An AngularJS 1.5 example usign redux",
  "scripts": {
    "build": "webpack",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:css": "stylelint 'app/**/*.css'",
    "lint:js": "eslint . --ext js",
    "lint:ts": "tslint 'app/**/*.ts' -e 'app/typings/**/*'",
    "postinstall": "npm run typings",
    "start": "npm run watch",
    "typings": "cd app && typings install",
    "watch": "webpack-dev-server"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nimedev/ng-redux-example.git"
  },
  "keywords": [
    "angularjs",
    "typescript",
    "webpack",
    "typings",
    "postcss",
    "redux",
    "ng-redux"
  ],
  "author": "nicolasmejiaco@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nimedev/ng-redux-example/issues"
  },
  "homepage": "https://github.com/nimedev/ng-redux-example#readme",
  "engines": {
    "npm": ">=3.10.8",
    "node": ">=6.9.1 <7.0.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "ng-redux": "3.4.0-beta.1",
    "niduscss-base": "1.0.1",
    "niduscss-components-button": "1.2.0",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "clean-webpack-plugin": "0.1.14",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "eslint": "3.10.2",
    "eslint-config-nimedev": "3.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "ip": "1.1.4",
    "json-loader": "0.5.4",
    "opener": "1.4.2",
    "postcss-calc": "5.3.1",
    "postcss-color-function": "2.0.1",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-import": "8.2.0",
    "postcss-loader": "1.1.1",
    "postcss-mixins": "5.4.0",
    "postcss-nested": "1.0.0",
    "pre-commit": "1.1.3",
    "redux-logger": "2.7.4",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "stylelint-config-nimedev": "7.0.0",
    "ts-loader": "1.2.2",
    "tslint": "4.0.1",
    "tslint-loader": "3.2.0",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.17.0",
    "webpack-validator": "2.2.9"
  }
}
